<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension id="e" name="XML Editors" point="org.jboss.tools.common.model.ui.xmlEditor">
      <xmlEditor 
            class="org.jboss.tools.jst.web.ui.editors.WebCompoundEditor" 
            contributorClass="org.jboss.tools.common.model.ui.texteditors.MultiPageContributor" 
            entities="FileCDIBeans,FileCDIBeans11" 
            icon="$nl$/images/xstudio/editors/beans_xml.png" 
            name="%editors.spring-beans.name">
      </xmlEditor>
   </extension>

	<extension point="org.jboss.tools.common.model.classes">
		<xclass id="org.jboss.tools.cdi.xml.ui.editor.form.CDIXMLFormLayoutData"
			class="org.jboss.tools.cdi.xml.ui.editor.form.CDIXMLFormLayoutData"/>
		<xclass id="org.jboss.tools.cdi.xml.ui.editor.form.CDINewClassWizardFactory"
			class="org.jboss.tools.cdi.xml.ui.editor.form.CDINewClassWizardFactory"/>
	</extension>


</plugin>
