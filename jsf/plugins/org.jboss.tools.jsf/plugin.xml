                  <equals value="org.jboss.tools.jsf.jsfnature"/>
               </iterate>
            </with>
            <with variable="element">
               <instanceof value="org.eclipse.core.resources.IFile"/>
            </with>
         </enablement>
      </moveParticipant>
   </extension>

